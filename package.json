{
  "name": "fastify-typescript",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "lint": "eslint --ext .ts,.tsx src",
    "test:ci": "CI=true NODE_ENV=test TZ='UTC' jest --config ./jest.config.js --passWithNoTests",
    "test": "run-p test:typescript test:dependencies test:ci",
    "test:dependencies": "npx depcheck",
    "test:complete": "pnpm run test:typescript && pnpm run test && pnpm run test:dependencies",
    "test:typescript": "tsc --noEmit -p ./tsconfig.json",
    "test:watch:coverage": "NODE_ENV=test jest --coverage --watchAll",
    "test:watch": "NODE_ENV=test TZ='UTC' jest --watchAll --detectOpenHandles",
    "postinstall": "husky install",
    "start": "NODE_ENV=production node dist/index.js",
    "build": "rm -rf dist && node esbuild.mjs",
    "dev": "NODE_ENV=development ts-node-dev src/index.ts"
  },
  "keywords": [],
  "author": "Dmitry Melnikov <d.dash.mv@gmail.com",
  "license": "ISC",
  "dependencies": {
    "@fastify/caching": "^8.0.1",
    "@fastify/compress": "^6.1.0",
    "@fastify/cors": "^8.0.0",
    "@fastify/helmet": "^9.1.0",
    "@fastify/type-provider-json-schema-to-ts": "^2.0.0",
    "fastify": "^4.3.0",
    "json-schema-to-ts": "^2.5.4"
  },
  "devDependencies": {
    "@getify/eslint-plugin-proper-arrows": "11.0.3",
    "@types/jest": "28.1.6",
    "@types/node": "18.6.0",
    "@typescript-eslint/eslint-plugin": "5.30.7",
    "@typescript-eslint/parser": "5.30.7",
    "depcheck": "1.4.3",
    "esbuild": "^0.14.50",
    "eslint": "8.20.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-async-await": "0.0.0",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "26.6.0",
    "eslint-plugin-json": "3.1.0",
    "eslint-plugin-markdown": "3.0.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-promise": "6.0.0",
    "eslint-plugin-ramda": "2.5.1",
    "get-port": "5",
    "husky": "8.0.1",
    "jest": "28.1.3",
    "jest-watch-typeahead": "2.0.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.7.1",
    "ts-jest": "28.0.7",
    "ts-node-dev": "2.0.0",
    "typescript": "^4.7.4"
  }
}
